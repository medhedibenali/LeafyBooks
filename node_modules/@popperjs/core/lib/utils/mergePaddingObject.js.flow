// @flow
import type { SideObject } from '@popperjs/core/lib/types';
import getFreshSideObject from '@popperjs/core/lib/utils/getFreshSideObject';

export default function mergePaddingObject(
  paddingObject: $Shape<SideObject>
): SideObject {
  return {
    ...getFreshSideObject(),
    ...paddingObject,
  };
}
